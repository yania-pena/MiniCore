<%- include ../layouts/cabecera.ejs %> 

    <div class = 'container mt-4'>
        <h1>Resultados</h1>
        <div class="card border-secondary">
            <div class="card-body">
                <table id="resultados2" class="table table-bordered table-striped text-center mt-5">
                    <thead>
                        <tr class="bg-dark text-white">
                            <th scope="col">Nombre</th>
                            <th scope="col">Fecha de Expiración</th>
                            <th scope="col">Tipo Pase</th>
                            <th scope="col">Pases</th>
                            <th scope="col">Cupos</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% datos.forEach((pase) => { %> 
                        <tr>
                            <td><%= pase.nombre %></td>
                            <td><%= pase.expiracion%></td>
                            <td><%= pase.tipo_pase%></td>
                            <td><%= pase.pase%></td>
                            <td><%= pase.cupo%></td>
                        </tr>
                        <% }) %> 
                    </tbody>
                </table>
                <a class="btn btn-primary" href="http:/pases/comparacionfecha/">Atrás</a>
            </div>
        </div>
        
    </div>
    <%- include ../layouts/pie.ejs %> 